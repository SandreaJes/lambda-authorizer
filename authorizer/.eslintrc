{
  "extends": ["eslint:recommended", "google","plugin:prettier/recommended"],
  "rules": {
    "prettier/prettier": "error",
    "max-len": [2, 120, 8],
    "no-console": 0,
    "no-invalid-this": "off"
  },
  "env": {
    "mocha": true,
    "amd": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "jsx": true
    }
  }
}
